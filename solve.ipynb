    "def solve(n):\n",
    "    # -> (a,b,c)\n",
    "    if n == 1:\n",
    "        return 1,1,1\n",
    "    else:\n",
    "        a0,b0,c0 = solve(n-1)\n",
    "        a = b0+c0\n",
    "        b = a0+c0\n",
    "        c = a0+b0+c0\n",
    "        return a,b,c\n",
    "a,b,c = solve(5)\n",
    "print(a+b+c)"
   ]
